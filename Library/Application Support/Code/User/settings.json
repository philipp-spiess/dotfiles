// Place your settings in this file to overwrite the default settings
{
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.fontFamily": "Fira Code",
  // Newline
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  // Excluded for Search
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/coverage": true,
    "**/dist": true,
    "web_test/_build": true,
    "**/*.gif": true,
    "**/*.png": true,
    "build/*": true
  },
  "editor.rulers": [72, 80, 100],
  "cSpell.userWords": [
    "affine",
    "backend",
    "codeownership",
    "Emscripten",
    "entrypoints",
    "esbuild",
    "FREETEXT",
    "frontmatter",
    "gitserver",
    "graphqlutil",
    "hashids",
    "jaccard",
    "KEYCODE",
    "linaria",
    "nicolasdular",
    "philipp",
    "philippspiess",
    "polyfilled",
    "pspdfkit",
    "rasterized",
    "recomponent",
    "rect",
    "rects",
    "Regexes",
    "Sourcegraph",
    "spiess",
    "Substate",
    "Truthy",
    "Uncomment",
    "Unmount",
    "WASM"
  ],
  "files.associations": {
    "*.tmpl": "ejs"
  },
  "atomKeymap.promptV3Features": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "window.nativeTabs": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.renderLineHighlight": "none",
  "editor.scrollbar.horizontal": "hidden",
  "explorer.openEditors.visible": 0,
  "workbench.iconTheme": null,
  "workbench.editor.showIcons": false,
  "vscode_custom_css.imports": [
    "file:///Users/philipp/dev/dotfiles/vscode-custom-css.css"
  ],
  "vscode_custom_css.policy": true,
  "vscode_custom_css.statusbar": true,
  "cSpell.showStatus": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "github.hosts": [
    {
      "host": "https://github.com",
      "token": "system"
    }
  ],
  "breadcrumbs.enabled": true,
  "workbench.startupEditor": "welcomePage",
  "editor.formatOnSave": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // How many spaces do you wish the indentation was? The extension will *try* to match this.
  "stretchySpaces.targetIndentation": 2,
  // For which languages Stretchy Spaces should be activated (if empty it means all).
  "stretchySpaces.includedLanguages": ["typescript"], // for example ["nim", "nims", "python"]
  // For which languages Stretchy Spaces should be deactivated (if empty it means none).
  "stretchySpaces.excludedLanguages": [],
  "stretchySpaces.updateDelay": 10,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.toolsManagement.autoUpdate": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "server": "localhost",
      "port": 5432,
      "driver": "PostgreSQL",
      "name": "localhost",
      "database": "sourcegraph",
      "username": "sourcegraph",
      "password": "sourcegraph"
    }
  ],
  "editor.guides.indentation": false,
  "editor.codeActionsOnSave": {},
  "github.copilot.enable": {
    "*": false,
    "plaintext": true,
    "markdown": false,
    "scminput": false,
    "yaml": true
  },
  "sourcegraph.basePath": "/Users/philipp/dev/",
  "liveshare.presence": true,
  // "http.proxyAuthorization": null,
  // "http.proxySupport": "off",
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[Log]": {
    "editor.fontSize": 12
  },
  "extensions.ignoreRecommendations": true,
  "editor.stickyScroll.enabled": true,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "window.commandCenter": true,
  "git.autofetch": true,
  "editor.fontLigatures": false,
  "editor.fontWeight": "300",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.accessibilitySupport": "off",
  "git.openRepositoryInParentFolders": "never",
  "debug.onTaskErrors": "debugAnyway",
  "gitlens.codeLens.enabled": false,
  // "github.copilot.advanced": {
  //   "debug.testOverrideProxyUrl": "http://localhost:3000",
  //   "debug.overrideProxyUrl": "http://localhost:3000"
  // },
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.inlineSuggest.enable": true,
  "files.watcherExclude": {
    "**/.bloop": true,
    "**/.metals": true,
    "**/.ammonite": true
  },
  "remote.SSH.defaultExtensions": ["gitpod.gitpod-remote-ssh"],
  "cody.debug.enable": true,
  "cody.debug.verbose": true,
  "cody.experimental.chatPredictions": true,
  "cody.experimental.inline": false,
  "workbench.colorTheme": "GitHub Dark Default",
  "cody.autocomplete.enabled": true,
  "cody.inlineChat.enabled": false,
  "cody.autocomplete.experimental.trhttps://github.com/sourcegraph/cody/issues/667iggerMoreEagerly": true,
  "tabnine.experimentalAutoImports": true,
  // "http.proxy": "socks5://192.168.0.221:9090"
  "cody.autocomplete.advanced.provider": "unstable-fireworks",
  "cody.autocomplete.advanced.model": "starcoder-16b",
  // "cody.autocomplete.advanced.accessToken": "nxr4KoCLBYPhGH7mEngiiBQkva7IQTbkJySZ08q0ipD1ZmdJ",
  // "cody.autocomplete.advanced.serverEndpoint": "https://api.fireworks.ai/inference/v1/completions",

  "cody.dev.openAutocompleteTraceView": true,
  "cody.dev.openOutputConsole": true,
  "cody.experimental.editorTitleCommandIcon": true,
  "pieces.setCopilotLocation": true,
  "cody.autocomplete.experimental.graphContext": true,
  "cody.experimental.commandLenses": false,
  "window.zoomLevel": 1,
  // "cody.autocomplete.advanced.provider": "unstable-fireworks",
  "editor.inlineSuggest.suppressSuggestions": true,
  // "cody.autocomplete.experimental.syntacticPostProcessing": false
  // "editor.foldingStrategy": "indentation"
}
