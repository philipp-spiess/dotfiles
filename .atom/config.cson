"*":
  autosave:
    enabled: true
  core:
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".flowconfig"
        ".hgrc"
      ]
      "source.json": [
        ".arcconfig"
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    disabledPackages: [
      "wrap-guide"
      "tree-view"
    ]
    telemetryConsent: "limited"
  docblockr:
    simple_mode: true
  editor:
    fontFamily: "Source Code Pro, Menlo, Monaco, 'Courier New', monospace"
    fontSize: 16
    invisibles: {}
    preferredLineLength: 100
  "exception-reporting":
    userId: "d9049b40-7134-070e-fb29-238e601d443e"
  linter: {}
  "linter-eslint": {}
  "linter-flow":
    executablePath: "/Users/philipp/dev/PSPDFKit-Web/node_modules/.bin/flow"
  "linter-ui-default": {}
  "multi-wrap-guide":
    locked: true
  nuclide:
    "nuclide-buck":
      compileErrorDiagnostics: false
      suggestTaskRunner: false
    "nuclide-clang":
      enableDefaultFlags: false
    "nuclide-ctags":
      disableWithHack: false
    "nuclide-debugger-php":
      stopOneStopAll: false
    "nuclide-diagnostics-store":
      consumeLegacyLinters: false
      legacyLintOnTheFly: false
    "nuclide-diff-view":
      shouldDockPublishView: false
    "nuclide-distraction-free-mode": {}
    "nuclide-file-tree":
      showOpenFiles: false
    "nuclide-flow":
      canUseFlowBin: true
    "nuclide-format-js":
      nuclideFixHeader: false
      requiresAddMissingRequires: false
      requiresAddMissingTypes: false
      requiresFormatRequires: false
      requiresRemoveUnusedRequires: false
      requiresRemoveUnusedTypes: false
      requiresTransferComments: false
    "nuclide-home":
      showHome: false
    "nuclide-objc":
      enableAutomaticSquareBracketInsertion: false
    "nuclide-ocaml":
      enableDiagnostics: false
    "nuclide-python":
      autocompleteArguments: false
      enableLinting: false
      showGlobalVariables: false
    use:
      "nuclide-datatip": false
      "nuclide-debugger-native": false
      "nuclide-debugger-node": false
      "nuclide-debugger-php": false
      "nuclide-home": false
      "nuclide-ios-simulator-logs": false
      "nuclide-objc": false
      "nuclide-ocaml": false
      "nuclide-react-native": false
      "nuclide-react-native-inspector": false
  "spell-check":
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "source.js.jsx"
      "source.c"
    ]
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".ansi.text":
  editor:
    preferredLineLength: 100
".hack.html.text":
  editor:
    preferredLineLength: 100
".hackfragment.source":
  editor:
    preferredLineLength: 100
".hg-commit.text":
  editor:
    preferredLineLength: 100
".null-grammar.plain.text":
  editor:
    preferredLineLength: 100
  "multi-wrap-guide":
    columns: [
      80
      100
    ]
".source.swift":
  editor:
    preferredLineLength: 100
